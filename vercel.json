{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/swagger-ui-init.js",
      "dest": "src/server.js"
    },
    {
      "src": "/api-docs/swagger-ui-bundle.js",
      "dest": "src/server.js"
    },
    {
      "src": "/api-docs/swagger-ui-standalone-preset.js",
      "dest": "src/server.js"
    },
    {
      "src": "/api-docs/swagger-ui.css",
      "dest": "src/server.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 